
@{
    Layout = null;
    
}

@model IEnumerable<CalledThat.ViewModels.TeamViewModel>
    

    <!DOCTYPE html>

    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>UnityTest</title>
    </head>
    <body>
        <div>
            @foreach(var item in Model.OrderBy(itm => itm.TeamName))
            {
                <table>
                    <tr>
                        <td>@item.TeamName</td>
                        <td><img src="@item.BadgeUrl" width="100" height="100" /></td>
                    </tr>
                </table>
            }
        </div>
    </body>
</html>
